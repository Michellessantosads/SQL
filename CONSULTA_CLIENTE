CREATE PROCEDURE CONSULTACLIENTE
    @IDCLIENTE INT = NULL,
    @NOME NVARCHAR(100) = NULL,
    @SOBRENOME NVARCHAR(100) = NULL,
    @EMAIL NVARCHAR(255) = NULL,
	@CPF BIGINT = NULL 
AS
BEGIN
    SELECT 
		ID_CLIENTE
		,NOME
		,SOBRENOME
		,EMAIL
		,CPF
		,ENDERECO
		,DATA_CADASTRO 
	FROM 
		CLIENTES 
	WHERE 
		ID_CLIENTE = @IDCLIENTE 
		OR NOME = @NOME
		OR SOBRENOME = @SOBRENOME
		OR EMAIL = @EMAIL
		OR CPF = @CPF
END;
